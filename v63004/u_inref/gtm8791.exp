set timeout 60
spawn /usr/local/bin/tcsh -f

send -- "\$gtm_dist/lke\r"

expect -exact "LKE>"
# Send <Ctrl-Z>
send -- "\x1A\r"

expect -exact "Suspended (signal)"
send -- "fg\r"

expect -exact "LKE>"
send -- "exit\r" 	#exits from the LKE prompt

expect -exact ">"
send -- "exit\r"	#exits from the tcsh shell

