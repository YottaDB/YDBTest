set timeout 120
spawn $env(SHELL)
send -- "set prompt=\"READT>\"\r"
expect "READT>"
send -- "gtm\r"
expect "YDB>"
send -- "set \$etrap=\"use \$principal:width=132 zshow \"\"v\"\"\"\r"
expect "YDB>"
send -- "set a=\$get(^ABC)\r"
expect "YDB>"
send -- "read x:10\r"
expect "YDB>"
send -- "zshow \"V\"\r"
expect "YDB>"
send -- halt\r
expect "READT>"
