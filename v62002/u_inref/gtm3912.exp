set timeout 120
spawn $env(SHELL)
send -- "set prompt=\"ZSHOWD>\"\r"
expect "ZSHOWD>"
send -- "gtm\r"
expect "YDB>"
send -- "set \$etrap=\"use \$principal:width=132 zshow \"\"v\"\"\"\r"
expect "YDB>"
send -- "set x=\"the quick brown fox jumped over the lazy dog\"\r"
expect "YDB>"
send -- "use \$principal:width=11\r"
expect "YDB>"
send -- "zshow \"D\"\r"
expect "YDB>"
send -- "use \$principal:width=10\r"
expect "YDB>"
send -- "zwrite\r"
expect "YDB>"
send -- halt\r
expect "ZSHOWD>"
